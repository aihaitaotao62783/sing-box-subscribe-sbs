{
  "log": {
    "level": "info",
    "timestamp": true
  },
  "experimental": {
    "clash_api": {
      "external_controller": "127.0.0.1:9090",
      "external_ui": "ui",
      "secret": "",
      "external_ui_download_url": "https://hubproxy.sky-moon.top:2053/https://github.com/MetaCubeX/metacubexd/archive/refs/heads/gh-pages.zip",
      "external_ui_download_detour": "Proxy",
      "default_mode": "global"
    },
    "cache_file": {
      "enabled": true,
      "store_fakeip": true
    }
  },
  "dns": {
    "servers": [
      {
        "tag": "remote",
        "type": "https",
        "server": "8.8.8.8",
        "detour": "Proxy"
      },
      {
        "tag": "local",
        "type": "https",
        "server": "223.5.5.5",
        "detour": "direct"
      }
    ],
    "rules": [
      { "clash_mode": "global", "server": "remote" },
      { "clash_mode": "direct", "server": "local" }
    ],
    "strategy": "prefer_ipv4"
  },
  "inbounds": [
    {
      "tag": "tun-in",
      "type": "tun",
      "address": ["172.19.0.0/30"],
      "stack": "system",
      "auto_route": true,
      "strict_route": true,
      "sniff": true
    },
    {
      "tag": "mixed-in",
      "type": "mixed",
      "listen": "127.0.0.1",
      "listen_port": 7890,
      "sniff": true
    }
  ],
  "outbounds": [
    {
      "tag": "Proxy",
      "type": "selector",
      "outbounds": ["Manual-Global", "Manual-China", "direct", "{all}"]
    },
    {
      "tag": "Manual-Global",
      "type": "selector",
      "outbounds": ["{all}"],
      "filter": [
        { "action": "include", "keywords": ["HK|TW|SG|JP|US|香港|台湾|新加坡|日本|美国"] }
      ]
    },
    {
      "tag": "Manual-China",
      "type": "selector",
      "outbounds": ["{all}"],
      "filter": [
        { "action": "include", "keywords": ["CN|中国|国内|移动|联通|电信"] }
      ]
    },
    { "type": "direct", "tag": "direct" }
  ],
  "route": {
    "auto_detect_interface": true,
    "default_domain_resolver": {
      "server": "local"
    },
    "rules": [
      { "protocol": "dns", "action": "hijack-dns" },
      { "clash_mode": "direct", "outbound": "direct" },
      { "clash_mode": "global", "outbound": "Proxy" }
    ],
    "final": "Proxy"
  }
}